{
  "name": "bolibro-rental-server",
  "version": "1.0.0",
  "description": "Bolibro Realty Property Management Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:debug": "ls -la && ls -la dist || echo 'dist not found' && node server.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "mkdir -p dist && tsc -p tsconfig.json",
    "postinstall": "if [ -f ./prisma/schema.prisma ]; then npx prisma generate --schema=./prisma/schema.prisma; else echo 'Skipping Prisma commands, schema not found'; fi",
    "lint": "eslint . --ext .ts",
    "seed": "ts-node prisma/seed.ts",
    "test-supabase": "node src/scripts/test-supabase.js",
    "migrate-images": "ts-node src/scripts/migrateImagesToSupabase.ts",
    "create-buckets": "node src/scripts/create-supabase-buckets.js",
    "ping-db": "node src/scripts/run-database-ping.js",
    "ping-storage": "node src/scripts/ping-supabase-storage.js",
    "ping-scheduler": "node src/scripts/ping-supabase.js",
    "ping-scheduler-storage": "node src/scripts/ping-supabase.js --skip-db"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@supabase/supabase-js": "^2.49.4",
    "@terraformer/wkt": "^2.2.1",
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.3",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "form-data": "^4.0.2",
    "nodemon": "^3.0.1",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
